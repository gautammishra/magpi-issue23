            <para LINESP="28"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="20" FCOLOR="Black" FSHADE="100" CH="Turn your Raspberry Pi into a"/>
            <para LINESP="28"/>
            <ITEXT FONT="FreeSans Bold" FONTSIZE="20" FCOLOR="Black" FSHADE="100" CH="XMPP Chat Server"/>
